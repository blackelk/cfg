[pull]
    rebase = false

[alias]
    a = add
    b = branch
    c = commit
    ch = checkout
    d = diff
    f = fetch
    l = log
    m = merge
    s = status

[color.status]
    added = "#66cc66"
    untracked = "#cc66cc"
    #untracked = "#cc6666"
    changed = "#cc6666"

[color.diff]
    oldMoved = "#cc6666"
    newMoved = "#66cc66"
    old = "#cc6666"
    new = "#66cc66"

[color.diff]
    oldMoved = "#bb6666"
    newMoved = "#66bb66"
    old = "#bb6666"
    new = "#66bb66"
